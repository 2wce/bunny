{
  "name": "@2wce/create-bunny",
  "module": "src/cli.ts",
  "author": {
    "name": "Kudakwashe Mupeni"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kudamupeni/bunny.git"
  },
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "create-bunny": "dist/cli"
  },
  "scripts": {
    "dev": "bun src/cli.ts",
    "build": "bun build --compile ./src/cli.ts --outfile=dist/cli",
    "release": "npx release publish"
  },
  "devDependencies": {
    "@ossjs/release": "^0.8.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/async-retry": "^1.4.6",
    "@types/fs-extra": "^11.0.2",
    "@types/got": "^9.6.12",
    "@types/gradient-string": "^1.1.3",
    "@types/tar": "^6.1.6",
    "bun-types": "latest",
    "gradient-string": "^2.0.2",
    "restore-cursor": "^5.0.0",
    "semantic-release": "^22.0.1",
    "update-check": "^1.5.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "got": "^13.0.0",
    "inquirer": "^9.2.11",
    "rimraf": "^5.0.5",
    "tar": "^6.2.0",
    "tsup": "^7.2.0"
  },
  "release": {
    "branches": [
      {
        "name": "main",
        "prerelease": false
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "tag": "latest"
  }
}