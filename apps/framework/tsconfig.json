{
  "compilerOptions": {
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "types": ["bun-types"],
    "target": "ES2015",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "paths": {
      "@app/*": ["./app/*"],
      "@config/*": ["./config/*"],
      "@database/*": ["./database/*"],
      "@generators/*": ["./bin/internals/generators/*"],
      "@scripts/*": ["./bin/scripts/*"],
      "@utils/*": ["./utils/*"]
    }
  }
}
